{
  "name": "instagram-profiles-client",
  "distribution_id": " {} ",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s",
    "s3:bucket": "node -p \"require('./package').name\"",
    "s3:upload": "aws s3 sync ./public s3://instagram-profiles-client",
    "cf:id": "node -p \"require('./package').distribution_id\"",
    "cf:clear-cache": "aws cloudfront create-invalidation --distribution-id {} --paths \"/*\"",
    "deploy": "yarn build && yarn s3:upload && yarn cf:clear-cache"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "autoprefixer": "^10.2.4",
    "postcss": "^8.2.6",
    "postcss-load-config": "^3.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.45.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.6.8",
    "tailwindcss": "^2.0.3"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "sirv-cli": "^1.0.0"
  }
}
